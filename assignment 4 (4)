def fibonacci(n):
    fib_series = [0, 1]

    for i in range(2, n):
        next_number = fib_series[-1] + fib_series[-2]
        fib_series.append(next_number)

    return fib_series

# enter the value of 'n' to control the length of the Fibonacci series
n =int(input ("enter the NUM of terms of fib series: "))
result = fibonacci(n)
print(result)